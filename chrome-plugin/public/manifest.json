{
  "manifest_version": 3,
  "name": "Owl Esports",
  "version": "0.0.1",
  "web_accessible_resources": [
    {
      "resources": [
        "script.js"
      ],
      "matches": [
        "https://*/*"
      ]
    }
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*/*"
      ],
      "js": [
        "content.js"
      ],
      "css": []
    }
  ],
  "permissions": [
    "storage",
    "unlimitedStorage"
  ],
  "host_permissions": ["https://www.dotabuff.com/heroes/arc-warden"],
  "action": {
    "default_icon": "./images/owl-esports.png"
  }
}